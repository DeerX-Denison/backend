{
	"compilerOptions": {
		"types": ["jest", "node", "cors"],
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"target": "es2017",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"baseUrl": "src"
	},
	"compileOnSave": true,
	"exclude": ["node_modules"]
}
